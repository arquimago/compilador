package Main;

import Cmenos.*;

public class Main {

	public static void main(String[] args) {
		if (args.length > 0) { 
	         try { 
	            LexerTh lexer = new LexerTh (new IPushbackReader( 
	               new FileReader(args[0]), 1024));  
	         } 
	         catch (Exception e) { 
	            System.out.println (e) ; 
	         } 
	      } else { 
	         System.err.println("Use um arquivo em C- como argumento"); 
	         System.exit(1); 
	      } 

	}

}
